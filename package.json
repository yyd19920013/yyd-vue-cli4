{
    "name": "yyd-vue-cli4",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "amfe-flexible": "^2.2.1",
        "core-js": "^3.6.4",
        "postcss-px2rem": "^0.3.0",
        "vue": "^2.6.11",
        "vue-router": "^3.1.6",
        "vuex": "^3.1.3",
        "vux": "^2.9.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.3.0",
        "@vue/cli-plugin-eslint": "~4.3.0",
        "@vue/cli-plugin-router": "~4.3.0",
        "@vue/cli-plugin-vuex": "~4.3.0",
        "@vue/cli-service": "~4.3.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vux/loader": "^2.0.0-rc4",
        "px2rem-loader": "^0.1.9",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-vue": "^6.2.2",
        "less": "^3.11.1",
        "less-loader": "^6.1.0",
        "prettier": "^1.19.1",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "quotes": [
                1,
                "single"
            ],
            "indent": [
                1,
                4
            ],
            "no-trailing-spaces": 0,
            "newline-after-var": 0,
            "comma-dangle": 0,
            "eqeqeq": 0,
            "curly": 0
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}